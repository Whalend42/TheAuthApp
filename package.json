{
  "name": "theauthproject",
  "version": "1.0.0",
  "description": "A project for authentication",
  "main": "dist/app.js",
  "scripts": {
    "start": "node_modules/ts-node/dist/bin.js src/app.ts",
    "start:dev": "node_modules/nodemon/bin/nodemon.js",
    "test": "jest"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "author": "Whalend42",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/redis": "^2.8.20",
    "babel-jest": "^26.0.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.0.0",
    "graphql-yoga": "^1.18.3",
    "pg-promise": "^10.5.5",
    "redis": "^3.0.2"
  }
}
